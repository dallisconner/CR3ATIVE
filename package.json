{
  "name": "cr3ative",
  "version": "1.0.0",
  "description": "A place for creative people to connect.",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "if-env": "^1.0.4",
    "mongo": "^0.1.0",
    "mongoose": "^5.11.8",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dallisconner/CR3ATIVE.git"
  },
  "author": "Dallis Conner, Tristan Eastman, Allison Haver, Jesse Ziros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dallisconner/CR3ATIVE/issues"
  },
  "homepage": "https://github.com/dallisconner/CR3ATIVE#readme"
}
